    <script>
        // Synonym database (subset for MVP)
        const synonymDatabase = {
            "glad": {
                type: "adjektiv",
                synonyms: ["lykkelig", "tilfreds", "fornøjet", "munter", "opstemt", "i godt humør", "begejstret", "jublende", "strålende", "henrykt"],
                antonyms: ["trist", "ulykkelig", "nedtrykt", "ked af det", "melankolsk"],
                related: ["glæde", "lykke", "tilfredshed", "munterhed"],
                example: "Hun var så <strong>glad</strong> for gaven, at hun næsten græd af glæde."
            },
            "stor": {
                type: "adjektiv",
                synonyms: ["enorm", "kæmpe", "vældig", "betydelig", "omfattende", "massiv", "gigantisk", "kolossal", "imponerende", "anselig"],
                antonyms: ["lille", "bitte", "minimal", "ubetydelig"],
                related: ["størrelse", "omfang", "dimension"],
                example: "Det var et <strong>stort</strong> projekt, der krævede mange ressourcer."
            },
            "god": {
                type: "adjektiv",
                synonyms: ["fremragende", "fortræffelig", "udmærket", "fin", "glimrende", "fornem", "ypperlig", "fantastisk", "super", "pragtfuld"],
                antonyms: ["dårlig", "elendig", "ringe", "ussel"],
                related: ["godhed", "kvalitet", "dygtighed"],
                example: "Det var en <strong>god</strong> ide at tage paraplyen med."
            },
            "hurtig": {
                type: "adjektiv",
                synonyms: ["hastig", "rask", "kvikt", "lynhurtig", "ekspres", "flink", "behændig", "snar", "øjeblikkelig", "prompt"],
                antonyms: ["langsom", "træg", "sløv", "gradvis"],
                related: ["hastighed", "hurtighed", "tempo"],
                example: "Han løb <strong>hurtigt</strong> ned ad gaden for at nå bussen."
            },
            "smuk": {
                type: "adjektiv",
                synonyms: ["skøn", "dejlig", "flot", "køn", "yndig", "henrivende", "betagende", "nydelig", "prægtig", "elegant"],
                antonyms: ["grim", "hæslig", "uskøn"],
                related: ["skønhed", "elegance", "ynde"],
                example: "Solnedgangen var utrolig <strong>smuk</strong> den aften."
            },
            "klog": {
                type: "adjektiv",
                synonyms: ["intelligent", "begavet", "dygtig", "skarp", "vidende", "lærd", "vis", "indsigtsfuld", "opvakt", "skarptsindet"],
                antonyms: ["dum", "uvidende", "naiv", "tankeløs"],
                related: ["klogskab", "intelligens", "visdom"],
                example: "Det var et <strong>klogt</strong> træk at investere tidligt."
            },
            "trist": {
                type: "adjektiv",
                synonyms: ["sørgelig", "bedrøvelig", "melankolsk", "tungsindig", "nedtrykt", "ked af det", "sorgfuld", "veemodig", "dyster", "tungsindigt"],
                antonyms: ["glad", "munter", "lykkelig", "opstemt"],
                related: ["tristhed", "sorg", "melankoli"],
                example: "Filmen havde en <strong>trist</strong> slutning, der fik mange til at græde."
            },
            "nem": {
                type: "adjektiv",
                synonyms: ["let", "ubesværet", "simpel", "ligetil", "ukompliceret", "overskuelig", "bekvem", "enkel"],
                antonyms: ["svær", "vanskelig", "kompliceret", "besværlig"],
                related: ["lethed", "enkelhed", "simpelhed"],
                example: "Opskriften var <strong>nem</strong> at følge, selv for begyndere."
            },
            "arbejde": {
                type: "substantiv/verbum",
                synonyms: ["job", "beskæftigelse", "erhverv", "ansættelse", "virke", "indsats", "stræben", "slid", "møje"],
                antonyms: ["fritid", "hvile", "ferie", "afslapning"],
                related: ["arbejder", "arbejdsgiver", "arbejdsplads"],
                example: "Han elsker sit <strong>arbejde</strong> og glæder sig til at møde hver morgen."
            },
            "hus": {
                type: "substantiv",
                synonyms: ["bolig", "hjem", "bopæl", "residens", "ejendom", "bygning", "villa", "lejlighed"],
                antonyms: [],
                related: ["husstand", "huslig", "hjemlig"],
                example: "De købte et nyt <strong>hus</strong> i udkanten af byen."
            },
            "spise": {
                type: "verbum",
                synonyms: ["indtage", "fortære", "nyde", "gumle", "proppe sig", "smovse", "tage til sig", "ernære sig"],
                antonyms: ["faste", "sulte"],
                related: ["mad", "måltid", "spisning"],
                example: "Vi skal <strong>spise</strong> middag klokken seks."
            },
            "løbe": {
                type: "verbum",
                synonyms: ["sprinte", "rende", "trave", "jogge", "spurte", "haste", "ile", "fare"],
                antonyms: ["gå", "stå", "sidde", "hvile"],
                related: ["løber", "løbetur", "løbebane"],
                example: "Hun elsker at <strong>løbe</strong> i skoven om morgenen."
            },
            "tale": {
                type: "verbum",
                synonyms: ["snakke", "kommunikere", "konversere", "diskutere", "drøfte", "udveksle ord", "ytre sig", "formulere sig"],
                antonyms: ["tie", "lytte", "være stille"],
                related: ["samtale", "dialog", "ytring"],
                example: "De sad og <strong>talte</strong> sammen i flere timer."
            },
            "tænke": {
                type: "verbum",
                synonyms: ["overveje", "reflektere", "gruble", "fundere", "spekulere", "meditere", "analysere", "vurdere"],
                antonyms: ["handle", "reagere impulsivt"],
                related: ["tanke", "tænkning", "eftertanke"],
                example: "Lad mig <strong>tænke</strong> over det og vende tilbage."
            },
            "se": {
                type: "verbum",
                synonyms: ["kigge", "betragte", "skue", "iagttage", "observere", "studere", "stirre", "glane"],
                antonyms: ["overse", "ignorere"],
                related: ["syn", "udsigt", "blik"],
                example: "Kan du <strong>se</strong> skibet derude på havet?"
            }
        };

        // DOM elements
        const searchInput = document.getElementById('searchInput');
        const searchBtn = document.getElementById('searchBtn');
        const resultsDiv = document.getElementById('results');
        const historySection = document.getElementById('historySection');
        const historyList = document.getElementById('historyList');
        const clearHistoryBtn = document.getElementById('clearHistory');
        const themeToggle = document.getElementById('themeToggle');
        const suggestions = document.querySelectorAll('.suggestion');

        // History
        let searchHistory = JSON.parse(localStorage.getItem('synonymHistory') || '[]');

        function search(word) {
            const normalizedWord = word.toLowerCase().trim();
            if (!normalizedWord) return;

            // Add to history
            if (!searchHistory.includes(normalizedWord)) {
                searchHistory.unshift(normalizedWord);
                if (searchHistory.length > 10) searchHistory.pop();
                localStorage.setItem('synonymHistory', JSON.stringify(searchHistory));
            }

            // Find in database
            const result = synonymDatabase[normalizedWord];

            if (result) {
                let html = `
                    <div class="result-header">
                        <div class="result-word">${normalizedWord}</div>
                        <div class="result-type">${result.type}</div>
                    </div>
                    <div class="result-body">
                        <div class="synonym-group">
                            <div class="synonym-label">Synonymer</div>
                            <div class="synonyms">
                                ${result.synonyms.map(s => `<span class="synonym" data-word="${s}">${s}</span>`).join('')}
                            </div>
                        </div>
                `;

                if (result.antonyms.length > 0) {
                    html += `
                        <div class="synonym-group antonyms">
                            <div class="synonym-label">Antonymer (modsætninger)</div>
                            <div class="synonyms">
                                ${result.antonyms.map(s => `<span class="synonym" data-word="${s}">${s}</span>`).join('')}
                            </div>
                        </div>
                    `;
                }

                if (result.related.length > 0) {
                    html += `
                        <div class="synonym-group">
                            <div class="synonym-label">Relaterede ord</div>
                            <div class="synonyms">
                                ${result.related.map(s => `<span class="synonym" data-word="${s}">${s}</span>`).join('')}
                            </div>
                        </div>
                    `;
                }

                if (result.example) {
                    html += `<div class="example">${result.example}</div>`;
                }

                html += '</div>';
                resultsDiv.innerHTML = html;
            } else {
                resultsDiv.innerHTML = `
                    <div class="no-results">
                        <h3>Ingen resultater for "${normalizedWord}"</h3>
                        <p>Prøv et andet ord eller tjek stavningen.</p>
                    </div>
                `;
            }

            resultsDiv.style.display = 'block';
            updateHistory();

            // Add click handlers to new synonyms
            resultsDiv.querySelectorAll('.synonym').forEach(syn => {
                syn.addEventListener('click', () => {
                    searchInput.value = syn.dataset.word;
                    search(syn.dataset.word);
                });
            });
        }

        function updateHistory() {
            if (searchHistory.length === 0) {
                historySection.style.display = 'none';
                return;
            }

            historySection.style.display = 'block';
            historyList.innerHTML = searchHistory.map(word => 
                `<span class="history-item" data-word="${word}">${word}</span>`
            ).join('');

            historyList.querySelectorAll('.history-item').forEach(item => {
                item.addEventListener('click', () => {
                    searchInput.value = item.dataset.word;
                    search(item.dataset.word);
                });
            });
        }

        // Event listeners
        searchBtn.addEventListener('click', () => search(searchInput.value));
        searchInput.addEventListener('keypress', (e) => {
            if (e.key === 'Enter') search(searchInput.value);
        });

        suggestions.forEach(s => {
            s.addEventListener('click', () => {
                searchInput.value = s.dataset.word;
                search(s.dataset.word);
            });
        });

        clearHistoryBtn.addEventListener('click', () => {
            searchHistory = [];
            localStorage.removeItem('synonymHistory');
            updateHistory();
        });

        themeToggle.addEventListener('click', () => {
            const isDark = document.body.hasAttribute('data-theme');
            if (isDark) {
                document.body.removeAttribute('data-theme');
                themeToggle.textContent = '☀️';
            } else {
                document.body.setAttribute('data-theme', 'dark');
                themeToggle.textContent = '🌙';
            }
            localStorage.setItem('theme', isDark ? 'light' : 'dark');
        });

        // Init
        if (localStorage.getItem('theme') === 'dark') {
            document.body.setAttribute('data-theme', 'dark');
            themeToggle.textContent = '🌙';
        }
        updateHistory();
    </script>
